page {
  background: #F2F2F2;
}

.header {
  /* background: url('../../images/account-top.png') no-repeat; */
  width: 750rpx;
  height: 204rpx;
  background-size: cover;
  box-sizing: border-box;
  padding-top: 42rpx;
  padding-left: 30rpx;
  position: relative;
}
.user-bg{
  width: 750rpx;
  height: 204rpx;
  position: absolute;
  top:0;
  left: 0;
}
.header-l{
  width: 141rpx;
  position: relative;
  margin-right: 24rpx;
}
.sign-icon{
  position: absolute;
  right: 0;
  bottom: 0;
  width: 55rpx;
  height: 56rpx;
}
.user-icon{
  width: 141rpx;
  height: 141rpx;
 
}

.login-btn {
  margin-top: 55rpx;
