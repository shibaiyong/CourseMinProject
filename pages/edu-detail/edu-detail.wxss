button {
  position:relative;
  display:block;
  margin-left:auto;
  margin-right:auto;
  padding: 0 0 !important;
  padding-left:0;
  padding-right:0;
  box-sizing:border-box;
  font-size:0;
  text-align:center;
  text-decoration:none;
  line-height:0;
  border-radius:5px;
  -webkit-tap-highlight-color:transparent;
  overflow:hidden;
  color:#000000;
  background-color:none;
}
button::after {
  border: none;
}
 button[size='mini']  {
  z-index:999;
  background-color: red;
    width: 50rpx ;
    margin-left: auto;
    margin-right: auto;
}
