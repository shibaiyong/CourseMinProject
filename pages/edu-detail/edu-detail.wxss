.successalert{
  width:100%;
  background:white;
  text-align: center;
  position:absolute;
  left:50%;
  bottom:0;
  transform:translateX(-50%);
  display:flex;
  justify-content: space-between;
  align-items: center;
  z-index: 100;
}

.successalert .shareposter,.successalert .message{
  display: flex;
  align-items: center;
  flex-direction: column;
  color: #333333;
  width:50%;
  text-align: center;
  margin:0;
  border-radius:0;
  padding:20rpx 0;
  background:white;
}
.successalert .shareposter text{
  color: #333333;
  font-weight: normal;
}
.successalert .shareposter image, .successalert .message image{
  display: block;
  width: 64rpx;
  height: 64rpx;
}

.successalert text{
  display: inline-block;
  font-size: 26rpx;
  line-height: 30rpx;
